{
    "version": "1.0.0",
    "configurations": [
        {
          "type": "node",
          "request": "launch",
          "name": "Jest",
          "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
          "args": [
            "${fileBasename}",
              "-i"
          ],
          "console": "integratedTerminal",
           "skipFiles": [
            "<node_internals>/**/*.js", "node_modules",
          ]
        }
      ],
  }